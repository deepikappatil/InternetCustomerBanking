<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Login">

	<listeners>
		<listener class-name="com.selenium.utilities.ExtentReporting"></listener>
	</listeners>

	<test thread-count="5" name="LoginTest">

		<parameter name="browser" value="firefox"></parameter>

		<classes>
			<class name="com.selenium.testCases.TC_LoginTest_001" />
			<class name="com.selenium.testCases.TC_LoginDDT_002" />
			<class name="com.selenium.testCases.TC_AddCustomerTest_003" />
		</classes>
	</test>

</suite> 
